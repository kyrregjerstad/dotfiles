    "vscodevim.vim": 1
  },
  "vim.leader": "<space>",
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["[", "d"],
      "commands": ["editor.action.marker.prev"]
    },
    {
      "before": ["]", "d"],
      "commands": ["editor.action.marker.next"]
    },
    {
      "before": ["<leader>", "r", "s"],
      "commands": ["editor.action.rename"]
    },
    {
      "before": ["<leader>", "r", "f"],
      "commands": ["editor.action.referenceSearch.trigger"]
    },
    {
      "before": ["<leader>", "l"],
      "commands": ["workbench.action.splitEditorRight"]
    },
    {
      "before": ["<leader>", "h"],
      "commands": ["workbench.action.splitEditorLeft"]
    },
    {
      "before": ["<leader>", "j"],
      "commands": ["workbench.action.splitEditorDown"]
    },
    {
      "before": ["<leader>", "m", "h"],
      "commands": ["workbench.action.moveEditorToLeftGroup"]
    },
    {
      "before": ["<leader>", "m", "l"],
      "commands": ["workbench.action.moveEditorToRightGroup"]
    },
    {
      "before": ["<leader>", "m", "k"],
      "commands": ["workbench.action.moveEditorToAboveGroup"]
    },
    {
      "before": ["<leader>", "m", "j"],
      "commands": ["workbench.action.moveEditorToBelowGroup"]
    },
    {
      "before": ["<leader>", "l"],
      "commands": [
        {
          "command": "editor.action.insertSnippet",
          "args": {
            "snippet": "console.log($1);"
          }
        }
      ]
    },
    {
      "before": ["<leader>", "t"],
      "commands": ["workbench.action.quickOpenPreviousRecentlyUsedEditor"]
    },
    {
      "before": ["<leader>", "f"],
      "commands": ["workbench.action.quickOpen"]
    },
    {
      "before": ["<leader>", "p"],
      "commands": ["workbench.action.showCommands"]
    }
  ],
  "vim.visualModeKeyBindingsNonRecursive": [
    {
      "before": ["<leader>", "l"],
      "commands": [
        {
          "command": "editor.action.insertSnippet",
          "args": {
            "snippet": "${TM_SELECTED_TEXT}\n\nconsole.log('${TM_SELECTED_TEXT}', ${TM_SELECTED_TEXT});"
          }
        },
        "extension.vim_escape",
        "cursorEnd"
      ]
    }
  ],
  "xstate.nestTypegenFiles": false
